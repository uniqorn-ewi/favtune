<p id="notice"><%= notice %></p>

<h1>Radio Stations</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @radio_stations.each do |radio_station| %>
      <tr>
        <td><%= link_to 'Show', radio_station %></td>
        <td><%= link_to 'Edit', edit_radio_station_path(radio_station) %></td>
        <td><%= link_to 'Destroy', radio_station, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Radio Station', new_radio_station_path %>
